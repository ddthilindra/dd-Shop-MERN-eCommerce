{
  "name": "dshop",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "data:import":"node backend/seeder",
    "data:destroy":"node backend/seeder -d"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddthilindra/dd-SHOP-MERN-eCommerce.git"
  },
  "author": "ddthilindra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ddthilindra/dd-SHOP-MERN-eCommerce/issues"
  },
  "homepage": "https://github.com/ddthilindra/dd-SHOP-MERN-eCommerce#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "mongoose": "^6.8.2"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}
